#include <iostream>
#define MAXSIZE 80
using namespace std;
//该程序未编写错误处理
typedef struct {
	int data[MAXSIZE] = { 2,1,5,2,3,1,6,2,4,1,7,9,1,5,8,3,5};
	int length = 17;
}SqList;
void DeleteX(SqList &L, int x) {
	//用k记录x出现的个数，将后边的元素移动k个位置，最后修改k的长度
	int k = 0;
	for (int i = 0; i < L.length; i++) {
		if (L.data[i] == x)			//若等于x,不用处理，直接循环到下一个，因为这里没有要求输出被删除元素的值之类的，所以直接覆盖就行
			k++;
		else
			L.data[i - k] = L.data[i];
	}
	L.length = L.length - k;
}




int main() {
	SqList L;
	int x = 1;
	cout << "Before" << endl;
	for (int i = 0; i < L.length; i++) {
		cout << L.data[i];
	}
	cout << endl;
	DeleteX(L, x);
	cout << "After" << endl;
	for (int j = 0; j < L.length; j++) {
		cout << L.data[j];
	}
	return 0;
}
